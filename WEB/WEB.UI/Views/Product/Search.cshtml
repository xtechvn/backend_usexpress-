<script>
    $('.list-category .xem-them').click(function () {
        $(this).parent().removeClass('open');
        if (!$(this).hasClass("active")) {
            $(this).addClass("active");
            $(this).parent().addClass('open');
        } else {
            $(this).removeClass("active");
            $(this).parent().removeClass('open');
        }
    });
</script>
