@model List<Entities.ViewModels.ArticleFeModel>
<div class="box_category box_hot_new">
    <h2 class="title">Xem nhiều nhất</h2>
    @foreach (var item in Model)
    {
        <article class="item-news">
            <div class="thumb-art">
                <a class="thumb thumb_5x3" href="@item.link" title="@item.title">
                    <img src="@item.image_169">
                </a>
            </div>
            <div class="content">
                <h3 class="title-news">
                    <a href="@item.link" title="@item.title">
                        @item.title
                    </a>
                </h3>
            </div>
        </article>
    }

</div>